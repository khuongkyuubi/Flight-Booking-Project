                </div>
                <div class="tabs-content">
                    <h4>Choose Your Direction:</h4>
                    <ul class="social-links">
                        <li><a href="https://www.facebook.com/groups/207964803013156/user/100002986066862">Find us on
                                <em>Facebook</em><i class="fa fa-facebook"></i></a>
                        </li>
                        <li><a href="https://www.youtube.com/channel/UCfAm3w13QJvjxbX3zpc9vCg">Our <em>YouTube</em>
                                Channel<i
                                        class="fa fa-youtube"></i></a></li>
                        <li><a href="http://instagram.com">Follow our <em>instagram</em><i
                                        class="fa fa-instagram"></i></a></li>
                    </ul>
                </div>
                <div class="page-direction-button">
                    <a href="contact.html"><i class="fa fa-phone"></i>Contact Us Now</a>
                </div>
            </div>
        </div>
        <div class="col-md-5 col-md-offset-1">
        </div>


        <div class="col-md-8">

            <section id="first-tab-group" class="tabgroup " style="padding-top: 0px !important">
                <div id="tab1">
                    <div class="submit-form">
                        <div class="row">
                            <div class="col-md-10">
                                <h3><i class="fa fa-plane" aria-hidden="true"
                                       style="color: DodgerBlue; font-size: 1.5em "></i> <%= flightInfo[0].flightID["departure"]["city"] %>
                                    (<%= flightInfo[0].flightID["departure"]["code"] %>) <i
                                            class="fa fa-long-arrow-right"
                                            aria-hidden="true"></i> <%= flightInfo[0].flightID["arrival"]["city"] %>
                                    (<%= flightInfo[0].flightID["arrival"]["code"] %>) </h3>
                            </div>
                            <div class="col-md-2">
                                <h3>
                                            <span><a href="javascript:void(0)"
                                                     onclick="">Flight Details</a></span>
                                </h3>
                            </div>
                            <div class="col-md-12">
                                <h4>
                                </h4>
                            </div>

                        </div>


                        <!--                            <form id="form-submit" action="/home/flight" method="GET">-->
                        <div class="row">
                            <div class="col-md-12">
                                <fieldset>
                                    <h5 class="bigger-font"><em><%= date %></em></h5>

                                </fieldset>
                            </div>


                            <div class="col-md-7">
                                <fieldset>
                                    <%
                                    let flightCode = flightInfo[0].flightID["airline"];
                                    let arlines = [
                                        {
                                            code: "QH",
                                            name: "Bamboo Airways"
                                        },
                                        {
                                            code: "VU",
                                            name: "Vietravel Airlines"
                                        },
                                        {
                                            code: "VNA",
                                            name: "Vietnam Airlines"
                                        },
                                        {
                                            code: "VJ",
                                            name: "VietJet Air"
                                        }
                                    ]

                                    let airline = arlines.find(airline => {
                                        return flightCode === airline.code
                                    }) || {code: "unknown", name: "unknown"};
                                    %>

                                    <label class="bigger-font"><%= airline.name %></label>
                                </fieldset>
                                <p><small>Promo</small></p>
                            </div>


                            <div class="col-md-3">
                                <div class="row">
                                    <div class="col-md-3">
                                        <label class="bigger-font"><%= flightInfo[0].flightID["STD"] %> </label>
                                    </div>
                                    <div class="col-md-4">
                                        <%
                                        // js code here
                                        let STD = flightInfo[0].flightID["STD"].split(":");
                                        STD.map(std => {
                                            std = parseInt(std);
                                        });
                                        let STA = flightInfo[0].flightID["STA"].split(":");
                                        STA.map(sta => {
                                            sta = parseInt(sta);
                                        });
                                        if (+STA[0] < +STD[0]) {
                                            STA[0] = 24 + STA[0];
                                        }
                                        let flightTime = [];
                                        flightTime[0] = STA[0] - STD[0];
                                        flightTime[1] = STA[1] - STD[1];
                                        %>
                                        <label class="bigger-font "><%= flightTime[0] %>h <%= flightTime[1] %>m</label>

                                    </div>
                                    <div class="col-md-3">
                                        <label class="bigger-font"><%= flightInfo[0].flightID["STA"] %> </label>

                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-3">
                                        <label class="bigger-font "><%= flightInfo[0].flightID["departure"]["code"] %> </label>
                                    </div>
                                    <div class="col-md-4 justify-content-center" style="text-align:center !important">
                                        <label class="bigger-font text-center"><i class="fa fa-long-arrow-right"
                                                                                  aria-hidden="true"></i></label>
                                    </div>
                                    <div class="col-md-3">
                                        <label class="bigger-font"><%= flightInfo[0].flightID["arrival"]["code"] %> </label>

                                    </div>
                                </div>


                                <!--                                        <p><small>e.g. +62812345678, for Country Code (+62) and Mobile No.-->
                                <!--                                                0812345678</small></p>-->
                            </div>
                        </div>
                        <!--                            </form>-->
                    </div>
                </div>
            </section>


        </div>


        <!--Right-->
        <div class="col-md-4 <!--col-md-offset-1-->">
            <section id="flight-sumary" class="tabgroup" style="padding-top: 0 !important">
                <div id="tab-right">
                    <div class="submit-form">
                        <h4> Price Details</h4>
                        <!--                            <form id="form-submit" action="/home/flight" method="GET">-->
                        <div class="row">
                            <!--Adult-->
                            <div class="col-md-8">
                                <fieldset>
                                    <label class="bigger-font" for="departure"><%= airline.name %> (Audult)
                                        x <%= quantityPassenger[0] %></label>
                                </fieldset>
                            </div>

                            <div class="col-md-4">
                                <fieldset>
                                    <label class="bigger-font">VND <%= man.toLocaleString("vi-VN") %></label>
                                </fieldset>
                            </div>

                            <!--Child-->
                            <div class="col-md-8">
                                <fieldset>
                                    <label class="bigger-font"><%= airline.name %> (Child)
                                        x <%= quantityPassenger[1] %></label>
                                </fieldset>
                            </div>

                            <div class="col-md-4">
                                <fieldset>
                                    <label class="bigger-font">VND <%= child.toLocaleString("vi-VN") %></label>
                                </fieldset>
                            </div>

                            <!--Infant - baby-->
                            <div class="col-md-8">
                                <fieldset>
                                    <label class="bigger-font"><%= airline.name %> (Infant)
                                        x <%= quantityPassenger[2] %></label>
                                </fieldset>
                            </div>

                            <div class="col-md-4">
                                <fieldset>
                                    <label class="bigger-font">VND <%= baby.toLocaleString("vi-VN") %></label>
                                </fieldset>
                            </div>

                            <div class="col-md-12">
                                <fieldset>
                                    <!--                                            Make line-->
                                    <h4></h4>
                                </fieldset>
                            </div>

                            <!--Price you pay-->
                            <div class="col-md-8">
                                <fieldset>
                                    <label class="bigger-font"> Price you pay</label>
                                </fieldset>
                            </div>

                            <div class="col-md-4">
                                <fieldset>
                                    <label class="bigger-font">VND <%= (man + child + baby).toLocaleString("vi-VN") %></label>
                                </fieldset>
                            </div>


                            <div class="col-md-12">
                                <fieldset>
                                    <!--                                            Make line-->
                                    <h4></h4>
                                </fieldset>
                            </div>

                            <div class="col-md-6"></div>
                            <div class="col-md-12">
                                <fieldset>
                                    <!--                                            <button  type="submit" id="form-submit" class="btn bigger-font">Continue</button>-->

                                    <a id="choose-link"
                                       href="/home/booking-flight/<%= flightInfo[0]._id %>-<%= quantityPassenger.join(".") %>">
                                        <button class="btn bigger-font">Continue</button>
                                    </a>
                                </fieldset>
                            </div>
                        </div>
                        <!--                            </form>-->
                    </div>
                </div>
            </section>
        </div>

